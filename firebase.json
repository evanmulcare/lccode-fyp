{
  "functions": {
    "source": "functions/functions",
    "timeoutSeconds": 300
  },
  "hosting": {
    "public": "dist/client/",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
